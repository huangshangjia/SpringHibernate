<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.manage.pub.vo">
    <class name="Staff" table="staff">
        <id name="staffId" column="staff_id"></id>
        <property name="staffCode" column="staff_code"></property>
        <property name="staffName" column="staff_name"></property>
        <property name="staffPwd" column="staff_pwd"></property>
        <property name="departmentId" column="department_id"></property>
    </class>
</hibernate-mapping>